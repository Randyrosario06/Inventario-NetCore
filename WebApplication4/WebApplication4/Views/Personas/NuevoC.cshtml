@model WebApplication4.Models.Clientes

@{ 
    ViewData["Title"] = "Nuevo";
}

<div class="text-center">
    <div>
        <h1>Registrar Cliente</h1>
    </div>
    <div>
        <form method="post">
            <div class="form-group-lg ">
                <div>
                    <label></label>
                </div>
                <div class="form-group-lg">
                    <input asp-for="Nombre" class=" text-center form-control" placeholder="Ingrese nombre" />
                    <span asp-validation-for="Nombre"></span>
                </div>
                <div class=" form-group-lg">
                    <label></label>
                </div>
                <div class="form-group-lg">
                    <input asp-for="Apellido" class=" text-center form-control" placeholder="Ingrese apellido" />
                </div>
                <div>
                    <label></label>
                </div>
                <div class="form-group-lg">
                    <input asp-for="Cedula" class=" text-center form-control" placeholder="Cedula" />
                    <span asp-validation-for="Cedula"></span>
                </div>
                <div class=" form-group-lg">
                    <label></label>
                </div>
                
                <div class=" form-group-lg">
                    <label></label>
                </div>

                <div>
                    <label>  </label>
                </div>
                <div class=" form-group-lg ">
                    <input type="submit" name="regist" value="Registrar" class="btn-default btn-lg form-control" />
                </div>
            </div>
        </form>
    </div>
</div>
